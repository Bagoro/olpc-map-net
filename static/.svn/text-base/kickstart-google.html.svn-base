<!DOCTYPE html>
<html>
	<head>
		<title>Kickstart with Google Maps</title>
		<script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false"></script>
		<script type="text/javascript">
function startMap(){
	var mymap = new google.maps.Map(document.getElementById("map_div"), {
		zoom: 11,
		center: new google.maps.LatLng(42,-87),
		mapTypeId: google.maps.MapTypeId.HYBRID
	});
	var kmllayer = new google.maps.KmlLayer('http://mapmeld.appspot.com/olpcMAP/kml?region=Massachusetts',{map:mymap});
}
		</script>
		<style type="text/css">
table img{max-height:150px;max-width:150px;}
		</style>
	</head>
	<body onload="startMap()">
		<div id="map_div" style="width:600px;height:500px;"></div>
	</body>
</html>