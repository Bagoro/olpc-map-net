body{
	font-family:verdana,sans-serif;
	background-color:#004200;
}
div.header{
	margin-left:-5%;
	margin-top:-30px;
	margin-bottom:-30px;
	padding-top:10px;
	text-align:center;
	width:110%;
	background-color:#000000;
	color:#ffffff;
}
div.nav{
	background-color:white;
	font-weight:bold;
	text-align:center;
	width:110%;
	padding:6px;
	margin-left:-5%;
}
span.navoption{
	border:2px solid white;
	/*margin-left:8px;
	margin-right:8px;*/
	margin-left:12px;
	margin-right:12px;
	padding:4px;
}
span.navoption:hover{
	background-color:silver;
	border:2px solid black;
}
span.footoption{
	border:2px solid white;
	margin-left:8px;
	margin-right:8px;
	padding:4px;
}
span.footoption a{
	color:black;
}
span.footoption a:hover{
	color:blue;
}
div.submenu{
	background-color:white;
	border-bottom:1px solid black;
	text-align:left;
	margin-top:-1px;
	padding-top:10px;
}
div.submenu a{
	color:black;
	text-decoration:none;
}
a.smenu{
	padding:2px 10px;
	margin-bottom:3px;
}
a.smenu:hover{
	padding:3px 10px;
	margin-bottom:3px;
	background-color:#cccccc;
}
div.main{
	background-color:#CCC998;
	width:70%;
	margin-left:auto;
	margin-right:auto;
	min-height:1000px;
	padding-left:5%;
	padding-right:5%;
	padding-bottom:16pt;
}
h2{
	width:100%;
	border-bottom:1px solid darkgreen;
	color:darkgreen;
}
div.main img{
	margin-right:30px;
}